$(function(){new Swiper(".section-slide .swiper-container",{slidesPerView:1,speed:2e3,autoplay:{delay:2e3,disableOnInteraction:!1},pagination:{el:".swiper-pagination",clickable:!0,renderBullet:function(e,i){return'<div class="'+i+'"><div></div></div>'}}});$(".swiper-auto").hover(function(){this.swiper.autoplay.stop()},function(){this.swiper.autoplay.start()});var e=0;$(".newslider__slide").each(function(){e<$(this).outerHeight()&&(e=$(this).outerHeight())}),$(".newslider__slide").css({"min-height":e,"background-size":"cover"})});